{
  "author": "Ellinor Rapp",
  "dependencies": {
    "@excalidraw/prettier-config": "1.0.2",
    "prettier": "2.2.1"
  },
  "description": "Virgil: handwritten font that powers Excalidraw",
  "license": "OFL-1.1",
  "name": "virgil",
  "prettier": "@excalidraw/prettier-config",
  "scripts": {
    "cleanup": "node script/cleanup.js",
    "fix": "yarn fix:files && yarn fix:font",
    "fix:files": "yarn prettier --write",
    "fix:font": "yarn cleanup && prettier src/Virgil.vfj --parser json --write",
    "lint": "yarn lint:files && yarn lint:font",
    "lint:files": "yarn prettier --list-different",
    "lint:font": "prettier src/Virgil.vfj --parser json --list-different",
    "prettier": "prettier ."
  },
  "version": "1.0.0"
}
